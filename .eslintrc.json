{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/react-in-jsx-scope": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "no-alert": 0,
    "no-unused-expressions": 0,
    "react-hooks/rules-of-hooks": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}
